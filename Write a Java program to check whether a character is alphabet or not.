import java.util.*;

public class ifq7 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Please press any key: ");
        char ch = sc.next().charAt(0);
        if ((ch >= 'a' && ch <= 'z')
                || (ch >= 'A' && ch <= 'z')) {
            System.out.println(ch + " is alphabet:");
        } else
            System.out.println(ch + " is not alphabet:");

    }

}
